{
	"name": "boilerplate-ts",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@types/axios": "^0.14.0",
		"@types/jest": "24.0.16",
		"@types/js-cookie": "^2.2.2",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/node": "^12.6.8",
		"@types/react": "16.8.23",
		"@types/react-dom": "^16.8.5",
		"@types/react-redux": "^7.1.1",
		"@types/react-router-dom": "^4.3.4",
		"axios": "^0.19.0",
		"concurrently": "^4.1.1",
		"css-loader": "^3.1.0",
		"js-cookie": "^2.2.0",
		"jsonwebtoken": "^8.5.1",
		"material-table": "^1.44.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-router-dom": "^5.0.1",
		"react-scripts": "3.0.1",
		"redux-saga": "^1.0.5",
		"sass": "^1.17.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.23.1",
		"typesafe-actions": "^4.4.2",
		"typescript": "3.5.3"
	},
	"scripts": {
		"start": "concurrently --kill-others \"npm run sass\" \"npm run local\"",
		"local": "npm run sass:build && HTTPS=true react-scripts start",
		"build": "npm run sass:build && react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"sass": "sass --watch src/assets/scss:src/assets/css --style compressed",
		"sass:build": "rm -rf src/assets/css/app* && sass src/assets/scss:src/assets/css --style compressed",
		"react-scripts start": "start"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
